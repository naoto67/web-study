<div class="container">
  <%= form_with model: @user, url: register_path do |f| %>
    <div>
      名前: <%= f.text_field :name, placeholder: 'name' %>
    </div>
    <div>
      アカウント名:<%= f.text_field :id_name, placeholder: 'account_name' %>
    </div>
    <div>
      <%= f.submit '送信' %>
    </div>
  <% end %>
</div>
